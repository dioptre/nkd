<Window x:Class="NKD.Import.Client.UI.CollarList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CollarList" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <Button x:Name="buttonDismiss" Content="Dismiss" HorizontalAlignment="Left" Margin="207,10,0,0" Grid.Row="2" VerticalAlignment="Top" Width="75" Click="buttonDismiss_Click"/>
        <ListBox x:Name="listBoxCollars" HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Stretch" />
        <Label x:Name="labelTitleLabel" Content="Collars" HorizontalAlignment="Left" Margin="6,4,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
